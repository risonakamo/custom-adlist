        "#cc60d2"
    ],
    "bracket-pair-colorizer-2.excludedLanguages":["markdown"],

    "spellright.notificationClass":"error",
    "spellright.documentTypes": [],
    "search.useIgnoreFiles": false,
    "editor.autoClosingQuotes": "never",
    "workbench.activityBar.visible": false,
    "git-graph.combineLocalAndRemoteBranchLabels": false,
    "git-graph.showStatusBarItem": false,
    "editor.find.seedSearchStringFromSelection": false,
    "files.autoSave": "off",
    "workbench.editor.closeOnFileDelete": true,
    "winopacity.opacity": 240,
    "editor.renderWhitespace": "none",
    "editor.renderIndentGuides": false,
    "css.completion.completePropertyWithSemicolon": false,
    "less.completion.completePropertyWithSemicolon": false,
    "markdown.preview.doubleClickToSwitchToEditor": false,

    "remote.extensionKind": {
        "coenraads.bracket-pair-colorizer-2": "ui"
    },

    "diffEditor.renderSideBySide": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "git-graph.commitDetailsViewLocation": "Docked to Bottom",
    "git-graph.fetchAndPrune": true,
    "git-graph.openRepoToHead": true,

    "autoscroll.keepLastLineInCenter": false,
    "editor.find.autoFindInSelection": "multiline",

    "workbench.colorCustomizations": {
