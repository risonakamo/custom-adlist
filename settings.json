    "spellright.notificationClass":"error",
    "spellright.documentTypes": [],

    "winopacity.opacity": 240,
    "glassit.alpha":230,

    "git-graph.combineLocalAndRemoteBranchLabels": false,
    "git-graph.showStatusBarItem": false,
    "git-graph.commitDetailsViewLocation": "Docked to Bottom",
    "git-graph.fetchAndPrune": true,
    "git-graph.openRepoToHead": true,

    "autoscroll.keepLastLineInCenter": false,

